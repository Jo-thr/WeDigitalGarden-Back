  "keywords": [
    "laravel",
    "nova",
    "optimistdigital",
    "field"
  ],
  "authors": [
    {
      "name": "Tarvo Reinpalu",
      "email": "tarvoreinpalu@gmail.com",
      "role": "Developer"
    },
    {
